		<servlet-class>it.unisa.server.OliCilento.Servlet.ServletRegistrazione</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registrazione</servlet-name>
		<url-pattern>/ServletRegistrazione</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>InserimentoOlio</servlet-name>
		<servlet-class>it.unisa.server.OliCilento.Servlet.ServletInserimentoOlio</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>InserimentoOlio</servlet-name>
		<url-pattern>/ServletInserimentoOlio</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DisplayProduttori</servlet-name>
		<servlet-class>it.unisa.server.OliCilento.Servlet.ServletDisplayProduttori</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DisplayProduttori</servlet-name>
		<url-pattern>/ServletDisplayProduttori</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DisplayRisorse</servlet-name>
		<servlet-class>it.unisa.server.OliCilento.Servlet.ServletDisplayRisorse</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DisplayRisorse</servlet-name>
		<url-pattern>/ServletDisplayRisorse</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DettagliProduttore</servlet-name>
		<servlet-class>it.unisa.server.OliCilento.Servlet.ServletDettagliProduttore</servlet-class>
	</servlet>

	<servlet-mapping>
