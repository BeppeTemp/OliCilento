		<url-pattern>/ServletDettagliProduttore</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RicercaProduttori</servlet-name>
		<servlet-class>it.unisa.server.OliCilento.Servlet.ServletRicercaProduttori</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RicercaProduttori</servlet-name>
		<url-pattern>/ServletRicercaProduttori</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ModificaBiografica</servlet-name>
		<servlet-class>it.unisa.server.OliCilento.Servlet.ServletModificaBio</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ModificaBiografica</servlet-name>
		<url-pattern>/ServletModificaBio</url-pattern>
	</servlet-mapping>

	<!-- Welcome files -->
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>


</web-app>

